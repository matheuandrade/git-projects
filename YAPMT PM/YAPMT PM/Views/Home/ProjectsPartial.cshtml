@model IEnumerable<YAPMT_PM.Models.Project>

@{
    ViewBag.Title = "ProjectsPartial";
}

 <nav class="col-sm-3 col-md-2 d-none d-sm-block bg-light sidebar">
          <ul class="nav nav-pills flex-column" style="margin-top:70px;">
              @foreach (var item in Model) {
                <li class="nav-item">
                    <a class="nav-link" href="/Home/Project/@item.ID"> @Html.DisplayFor(modelItem => item.Name) <span class="sr-only">(current)</span></a>
                </li>
              }
          </ul>
        </nav>
